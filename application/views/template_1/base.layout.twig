<!DOCTYPE html>
<html lang="en">
{% spaceless %}
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1">
    <meta name="robots" content="index, follow" />
    <title>{{ site_name ~ ' - ' ~ head['title'] }}</title>

    {% if isHomepage %}
        <meta http-equiv="refresh" content="300">
    {% endif %}
    <!-- Meta info -->
    <meta name="keywords" content="{{ head['keyword'] }}" />
    <meta name="description" content="{{ head['description'] }}">
    <link rel="canonical" href="{{ head['canonical'] }}" />
    <link rel="alternate" hreflang="x-default" href= "{{ head['alternate'] }}" />
    {% if head['amphtml'] %}
        <link rel="amphtml" href="{{ head['amphtml'] }}">
    {% endif %}

    <link rel="shortcut icon" href="{{ base_url }}public/images/img/logo/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="{{ base_url }}public/images/img/logo/apple-touch-icon.png" />
    <link rel="apple-touch-icon" sizes="57x57" href="{{ base_url }}public/images/img/logo/apple-touch-icon-57x57.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="{{ base_url }}public/images/img/logo/apple-touch-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="{{ base_url }}public/images/img/logo/apple-touch-icon-76x76.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="{{ base_url }}public/images/img/logo/apple-touch-icon-114x114.png" />
    <link rel="apple-touch-icon" sizes="120x120" href="{{ base_url }}public/images/img/logo/apple-touch-icon-120x120.png" />
    <link rel="apple-touch-icon" sizes="144x144" href="{{ base_url }}public/images/img/logo/apple-touch-icon-144x144.png" />
    <link rel="apple-touch-icon" sizes="152x152" href="{{ base_url }}public/images/img/logo/apple-touch-icon-152x152.png" />
    <link rel="apple-touch-icon" sizes="180x180" href="{{ base_url }}public/images/img/logo/apple-touch-icon-180x180.png" />
    <link rel="manifest" href="{{ base_url }}public/images/img/logo/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ base_url }}public/images/img/logo/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@{{ domain_name }}">
    <meta name="twitter:title" content="{{ head['title'] }}">
    <meta name="twitter:description" content="{{ head['description'] }}">
    <meta name="twitter:image" content="{{ head['image'] }}">

    <!-- Open Graph data -->
    <meta property="og:title" content="{{ head['title'] }}" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{ head['canonical'] }}" />
    <meta property="og:image" content="{{ head['image'] }}" />
    <meta property="og:description" content="{{ head['description'] }}" />
    <meta property="og:site_name" content="{{ domain_name }}" />
    <meta property="fb:app_id" content="{{ config['facebook_app_id'] }}" />
    <meta name="pinterest-rich-pin" content="false" />

    {{ script['js'] | raw }}

    {% if script['gruntStyle'] %}
        <style>
            {{ script['gruntStyle'] | raw }}
        </style>
    {% endif %}

    {{ script['css_no_defer'] | raw }}
    <noscript id="deferred-styles">
        {{ script['css'] | raw }}
        <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.css" />
    </noscript>

    <script>
        baseUrl = '{{ base_url }}';
        var loadDeferredStyles = function() {
            var addStylesNode = document.getElementById("deferred-styles");
            var replacement = document.createElement("div");
            replacement.innerHTML = addStylesNode.textContent;
            document.body.insertBefore(replacement, document.body.childNodes[0]);
            addStylesNode.parentElement.removeChild(addStylesNode);
        };
        var raf = requestAnimationFrame || mozRequestAnimationFrame ||
                webkitRequestAnimationFrame || msRequestAnimationFrame;
        if (raf) raf(function() { window.setTimeout(loadDeferredStyles, 0); });
        else window.addEventListener('load', loadDeferredStyles);
    </script>

    {% if gruntStyleFile %}
        <style>
            {{ source(gruntStyleFile) }}
        </style>
    {% endif %}

    <script id="id_consent" async src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>

    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','{{ base_url }}analytics.js','ga');

        ga('create', 'UA-6586972-28', 'auto');
        ga('send', 'pageview');
    </script>
</head>
<body class="{{ bodyClass }}">

<div class="navbar">
    <div class="container">
        <div class="nav-container">
            <div style="float: left;">
                <a href="{{ base_url }}">
                    <div class="logo inline">
                        <img src="{{ config['icon_cslogo'] }}" alt="Coinschedule Logo">
                        <div class="logo-text"><span style="color: #77797D">Coin</span>Schedule</div>
                    </div>
                </a>

                <div class="mediasquares inline">
                    <div class="hide">
                        <a href="{{ base_url }}blog/" target="_blank">
                            <img src="{{ config['icon_blog'] }}" alt="Blog Link Icon">
                        </a>
                    </div>

                    <div>
                        <a href="http://bit.ly/2zY5yCj" target="_blank">
                            <img src="{{ config['icon_telegram'] }}" alt="Telegram Link Icon">
                        </a>
                    </div>

                    <div>
                        <a href="{{ base_url }}slack.html" target="_blank">
                            <img src="{{ config['icon_slack'] }}" alt="Slack Link Icon">
                        </a>
                    </div>

                    <div class="hide">
                        <a href="https://twitter.com/coinschedule" target="_blank">
                            <img src="{{ config['icon_twitter'] }}" alt="Twitter Link Icon">
                        </a>
                    </div>

                    <div class="hide">
                        <a href="https://www.facebook.com/Coinschedule-673455652847426/" target="_blank">
                            <img src="{{ config['icon_facebook'] }}" alt="Telegram Link Icon">
                        </a>
                    </div>

                    <div class="hide">
                        <a href="https://www.reddit.com/user/coinschedule/" target="_blank">
                            <img src="{{ config['icon_reddit'] }}" alt="Reddit Link Icon">
                        </a>
                    </div>

                    <div class="hide">
                        <a href="https://www.linkedin.com/company/coinschedule" target="_blank">
                            <img src="{{ config['icon_linkedin'] }}" alt="LinkedIn Link Icon">
                        </a>
                    </div>
                </div>
            </div>

            {% if ssUser %}
                <input type="checkbox" id="usermenu-trigger" class="usermenu-trigger" onChange="$('.nav-trigger').prop('checked', false);"/>
                <label for="usermenu-trigger" class="usermenubutton">
                    <span class="username">{{ ssUsername }}</span>
                    <span class="userinitial">{{ ssUserInit }}</span>
                </label>

                <div class="usermenu">
                    <div class="usermenu-item"><a href="{{ base_url }}profile.html"><img src="{{ config['icon_profile'] }}" class="inline">Profile</a></div>
                    <div class="usermenu-item"><a href="{{ base_url }}listings.html"><img src="{{ config['icon_listing'] }}" class="inline">Listings</a></div>
                    <div class="usermenu-item"><a href="{{ base_url }}logout"><img src="{{ config['icon_logout'] }}" class="inline">Logout</a></div>
                </div>
            {% endif %}

            <input type="checkbox" id="nav-trigger" class="nav-trigger" onChange="$('.usermenu-trigger').prop('checked', false);"/>
            <label for="nav-trigger">
                <span class="burger inline">
                  <span></span>
                  <span></span>
                  <span></span>
                </span>
            </label>

            <div class="nav inline">
                {% if isHomepage %}
                    <div class="nav-item inline"><a href="#">ICOs</a></div>
                    <div class="nav-item inline"><a href="#blog">Blog</a></div>
                    <div class="nav-item inline"><a href="#events">Events</a></div>
                {% else %}
                    <div class="nav-item inline"><a href="{{ base_url }}">ICOs</a></div>
                    <div class="nav-item inline"><a href="{{ base_url }}#blog">Blog</a></div>
                    <div class="nav-item inline"><a href="{{ base_url }}#events">Events</a></div>
                {% endif %}
                <div class="nav-item inline"><a href="{{ base_url }}icos.html">Results</a></div>
                <div class="nav-item inline"><a href="{{ base_url }}stats.html">Stats</a></div>
                <div class="nav-item inline"><a href="{{ base_url }}submit_entry.html">Submit</a></div>
                <div class="nav-item inline"><a href="{{ base_url }}advertise.html">Advertise</a></div>
                {% if not ssUser %}
                    <div class="nav-item inline"><a href="{{ base_url }}login.html">Login</a></div>
                {% endif %}
            </div>
        </div>
    </div>
</div>

<div class="container">
    {{ include(render) }}

    <div class="footer">
        <div class="footer-col inline top">
            <h4>Website</h4>
            <a href="{{ base_url }}">Home</a>
            <a href="{{ base_url }}about.html">About</a>
            <a href="{{ base_url }}submit_entry.html">Submit Entry</a>
            <a href="{{ base_url }}advertise.html">Advertise</a>
            {% if ssUser %}
                <a href="{{ base_url }}logout.html">Logout</a>
            {% else %}
                <a href="{{ base_url }}login.html">Login</a>
            {% endif %}
        </div>
        <div class="footer-col inline top">
            <h4>Tools</h4>
            <a href="{{ base_url }}icos.html">Results</a>
            <a href="{{ base_url }}stats.html">Stats</a>
            <a href="https://developers.coinschedule.com/" target="_blank">Developers (API)</a>
        </div>
        <div class="footer-col inline top">
            <h4>Media</h4>
            <a href="{{ base_url }}blog" target="_blank">Blog</a>
            <a href="{{ base_url }}press.html">Press Mentions</a>
            <a href="https://twitter.com/coinschedule" target="_blank">Twitter</a>
            <a href="https://www.facebook.com/Coinschedule-673455652847426/" target="_blank">Facebook</a>
            <a href="{{ base_url }}slack.html" target="_blank">Slack</a>
            <a href="https://www.reddit.com/user/coinschedule/" target="_blank">Reddit</a>
            <a href="https://www.linkedin.com/company/coinschedule" target="_blank">Linkedin</a>
        </div>
        <div class="footer-col inline top">
            <h4>Legal</h4>
            <a href="{{ base_url }}terms.html">Terms</a>
            <a href="{{ base_url }}disclaimer.html">Disclaimer</a>
            <a href="{{ base_url }}privacypolicy.html">Privacy Policy</a>
            <a href="{{ base_url }}cookies_policy.html">Cookie Policy</a>
        </div>
    </div>

    <div class="copyright">
        <div class="inline">Copyright &copy; {{ thisYear }} <a href="https://www.coinschedule.com">Coinschedule.</a></div>
        &nbsp;
        <div class="inline dmca">
            <a href="//www.dmca.com/Protection/Status.aspx?ID=18ff9b78-666a-445b-8b50-96af4895f2a0" title="DMCA.com Protection Status" class="dmca-badge" target="_blank">
                <img src="{{ base_url }}public/images/img/dmca-badge-w150-5x1-08.png" alt="DMCA.com Protection Status">
            </a>
            <script src="//images.dmca.com/Badges/DMCABadgeHelper.min.js"> </script>
        </div>
        <div class="stretch"></div>
    </div>

</div>

{% if reviveBanner %}
    <script async src="https://www.coinschedule.com/ads/www/delivery/asyncjs.php"></script>
    <script async src="//appsha1.cointraffic.io/js/?wkey=cNhyIu"></script>
{% endif %}

<script>
    window.cookieconsent.initialise({
        "palette": {
            "popup": {
                "background": "#252e39"
            },
            "button": {
                "background": "transparent",
                "text": "#0FBE7C",
                "border": "#0FBE7C"
            }
        },
        "position": "top",
        "content": {
            "message": "By using Coinschedule you agree to our <a class='a-cookie' href='{{ base_url }}terms.html'>Terms and Conditions</a>, <a class='a-cookie' href='{{ base_url }}disclaimer.html'>Disclaimer</a> and",
            "dismiss": "Don't show again for 30 days",
            "link": "Cookies Policy",
            "href": "{{ base_url }}cookies_policy.html"
        },
        "cookie": {
            "expiryDays": 30
        }
    })
</script>

{% endspaceless %}
</body>
</html>